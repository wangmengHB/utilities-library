{
	"compilerOptions": {
		"noImplicitAny": true,
		"experimentalDecorators": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"strictBindCallApply": true,
		"strictNullChecks": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": ".",
		"noEmit": true,
		"types": [],
		"paths": {},
		"module": "amd",
		"moduleResolution": "classic",
		"removeComments": false,
		"preserveConstEnums": true,
    "declaration": true,
    "lib": [
      "dom",
      "es2020"
    ],
    "target": "es5",
		"sourceMap": false, 
    "outDir": "./lib"
	},
	"include": [
		"src/base/*",
	],
	"exclude": [
		"node_modules/*"
	]
}
